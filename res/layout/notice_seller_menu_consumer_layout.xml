<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/notice_seller_menu_consume_layout"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <View
        android:id="@+id/noticeTopLineNew"
        android:layout_width="fill_parent"
        android:layout_height="1.0px"
        android:layout_marginTop="@dimen/tuan_detail_gap_height"
        android:background="@color/tuan_detial_line_color" />

    <include
        android:id="@+id/tuan_detail_notice"
        layout="@layout/details_text_layout" />

    <View
        android:id="@+id/noticeBottomLineNew"
        android:layout_width="fill_parent"
        android:layout_height="1.0px"
        android:layout_marginBottom="@dimen/tuan_detail_gap_height"
        android:background="@color/tuan_detial_line_color" />

    <View
        android:id="@+id/sellerInfoItemTopLineNew"
        android:layout_width="fill_parent"
        android:layout_height="1.0px"
        android:layout_marginTop="@dimen/tuan_detail_gap_height"
        android:background="@color/tuan_detial_line_color" />

    <include
        android:id="@+id/tuan_detail_seller_info_item"
        layout="@layout/tuan_detail_seller_info_item" />

    <View
        android:id="@+id/sellerInfoItemBottomLineNew"
        android:layout_width="fill_parent"
        android:layout_height="1.0px"
        android:layout_marginBottom="@dimen/tuan_detail_gap_height"
        android:background="@color/tuan_detial_line_color" />

    <View
        android:id="@+id/menuTopLineNew"
        android:layout_width="fill_parent"
        android:layout_height="1.0px"
        android:layout_marginTop="@dimen/tuan_detail_gap_height"
        android:background="@color/tuan_detial_line_color" />

    <include
        android:id="@+id/tuan_detail_menu_info"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        layout="@layout/tuan_detail_menu_info" />

    <View
        android:id="@+id/menuBottomLineNew"
        android:layout_width="fill_parent"
        android:layout_height="1.0px"
        android:layout_marginBottom="@dimen/tuan_detail_gap_height"
        android:background="@color/tuan_detial_line_color" />

    <View
        android:id="@+id/consumerTopLineNew"
        android:layout_width="fill_parent"
        android:layout_height="1.0px"
        android:layout_marginTop="@dimen/tuan_detail_gap_height"
        android:background="@color/tuan_detial_line_color" />

    <include
        android:id="@+id/tuan_detail_consumer_tips"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        layout="@layout/tuan_detail_consumer_tips" />

    <View
        android:id="@+id/consumerBottomLineNew"
        android:layout_width="fill_parent"
        android:layout_height="1.0px"
        android:layout_marginBottom="@dimen/tuan_detail_gap_height"
        android:background="@color/tuan_detial_line_color" />

</LinearLayout>